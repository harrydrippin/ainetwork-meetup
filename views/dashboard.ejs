<!DOCTYPE html>
<html>
  <head>
    <title>Dashboard</title>
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css" integrity="sha384-MCw98/SFnGE8fJT3GXwEOngsV7Zt27NXFoaoApmYm81iuXoPkFOJwJ8ERdknLPMO" crossorigin="anonymous">
  </head>
  <body>
      <div class="container">
        <h1>Dashboard</h1>
        <hr>
        <table class="table">
            <thead>
                <tr>
                <th scope="col">#</th>
                <th scope="col">이름</th>
                <th scope="col">Username</th>
                <th scope="col">E-mail</th>
                <th scope="col">Phone</th>
                <th scope="col">인증 시기</th>
                </tr>
            </thead>
            <tbody>
                <% for(var i=0; i < attendants.length; i++) { %>
                    <tr>
                    <th scope="row"><%= i + 1 %></th>
                    <td><%= attendants[i].name %></td>
                    <td><%= attendants[i].username %></td>
                    <td><%= attendants[i].email %></td>
                    <td><%= attendants[i].phone %></td>
                    <td><%= attendants[i].creationDate %></td>
                    </tr>
                <% } %>
            </tbody>
        </table>
      </div>
  </body>
</html>
